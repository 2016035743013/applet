page {
  width: 100%;
  height: 100%;
}
.goodAddEdit {
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  background: white;
}
.rect {
  width: 100%;
  height: 170rpx;
  background: #5A95FF;
  position: absolute;
  top: 0rpx;
  left: 0rpx;
}
.goodContent { 
  width: 90%;
  height: 90%;
  margin: 10rpx auto;
  position: relative;
}
.topText {
  width: 100%;
  height: 10%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: bold;
  color: white;

}
.bottomContent {
  width: 100%;
  height: 80%;
  background: white;
  box-shadow: 0 0 10px 1px rgba(0, 0, 0, 0.11);
  border-radius: 10rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}
.bottomContent>view {
  width: 80%;
  height: 80%;
}
.content>view {
  color: #ccc;
  font-size: 35rpx;
  margin-bottom: 20rpx;
}
.content>view:nth-child(2) {
  background: #F7F7F7;
  color: black;
  padding-left: 20rpx;
  box-sizing: border-box;
}
.content>view:nth-child(4) {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.content>view:nth-child(4)>view:nth-child(1), .content>view:nth-child(4)>view:nth-child(3) {
  width: 50rpx;
  height: 50rpx;
  background: #5A95FF;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
}
.content>view:nth-child(4)>view:nth-child(2)>image{
  width: 60rpx;
  height: 60rpx;
}
.content>view:nth-child(4)>view:nth-child(2) {
  display: flex;
  justify-content: center;
  align-items: center;
}
.content>view:nth-child(4) image {
  width: 40rpx;
  height: 40rpx;

}
.content>view:nth-child(5) {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content>view:nth-child(5)>image {
  width: 50rpx;
  height: 50rpx;
}
.content>view:nth-child(7) {
  width: 100%;
  height: 80rpx;
  color: white;
  background: #5A95FF;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
}
.chooseImage { 
  width: 100%;
  height: 200rpx;
  background: #ccc;
  display: flex;
  justify-content: center;
  align-items: center;
}
.chooseImage>image {
  width: 100rpx;
  height: 100rpx;
}

